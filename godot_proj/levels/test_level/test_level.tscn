[sub_resource type="GDScript" id="GDScript_x25rp"]
script/source = "extends Node

@export var _path: Path3D
@export var _level_life: Node

func _ready() -> void:
	_timer()

func _timer():
	await get_tree().create_timer(2.0).timeout
	_spawn_enemy()
	_timer()

func _spawn_enemy():
	var enemy := Factory.get_instance().spawn_square_enemy()
	enemy.progress = 0
	enemy.attack.connect(_level_life.apply_damage)
	_path.add_child(enemy)
"

[sub_resource type="GDScript" id="GDScript_gmgay"]
script/source = "extends Node

signal dead()

@export var max_life: float

var _current_life: float

func _ready() -> void:
	_current_life = max_life

func apply_damage(amount: float):
	_current_life -= amount
	MainLifeBar.get_instance().set_life(_current_life, max_life)
	if _current_life <= 0:
		dead.emit()
		print(\"Level Over\")
"

